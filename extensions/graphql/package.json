{
  "name": "@loopback/graphql",
  "description": "LoopBack's graphql integration",
  "version": "0.3.0",
  "keywords": [
    "LoopBack",
    "GraphQL"
  ],
  "license": "MIT",
  "author": "IBM Corp.",
  "copyright.owner": "IBM Corp.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
    "directory": "extensions/graphql"
  },
  "engines": {
    "node": "^10.16 || 12 || 14 || 15"
  },
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build": "lb-tsc",
    "clean": "lb-clean loopback-graphql*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "integration": "lb-mocha \"dist/__tests__/integration/**/*.js\"",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "unit": "lb-mocha \"dist/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf loopback-graphql*.tgz && tree package && npm run clean"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "peerDependencies": {
    "@loopback/boot": "^3.3.0",
    "@loopback/core": "^2.15.0"
  },
  "dependencies": {
    "@loopback/http-server": "^2.4.0",
    "apollo-server-express": "^2.22.2",
    "debug": "^4.3.1",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@loopback/build": "^6.3.0",
    "@loopback/eslint-config": "^10.1.0",
    "@loopback/repository": "^3.5.0",
    "@loopback/rest": "^9.2.0",
    "@loopback/testlab": "^3.3.0",
    "@types/debug": "^4.1.5",
    "@types/node": "^10.17.56",
    "class-transformer": "^0.4.0"
  }
}
