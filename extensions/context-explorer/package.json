{
  "name": "@loopback/context-explorer",
  "description": "Visualize context hierarchy, bindings, configurations, and dependencies",
  "version": "0.4.0",
  "keywords": [
    "LoopBack",
    "Explorer",
    "Context",
    "Binding"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp.",
  "copyright.owner": "IBM Corp.",
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
    "directory": "extensions/context-explorer"
  },
  "engines": {
    "node": "^10.16 || 12 || 14 || 15"
  },
  "scripts": {
    "build": "lb-tsc",
    "clean": "lb-clean loopback-explorer*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "verify": "npm pack && tar xf loopback-context-explorer*.tgz && tree package && npm run clean"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "public",
    "!*/__tests__"
  ],
  "peerDependencies": {
    "@loopback/core": "^2.15.0",
    "@loopback/rest": "^9.2.0"
  },
  "dependencies": {
    "ts-graphviz": "^0.14.0",
    "viz.js": "^2.1.2"
  },
  "devDependencies": {
    "@loopback/build": "^6.3.0",
    "@loopback/eslint-config": "^10.1.0",
    "@loopback/testlab": "^3.3.0",
    "@types/node": "^10.17.56"
  }
}
