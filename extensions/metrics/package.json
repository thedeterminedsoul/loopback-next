{
  "name": "@loopback/metrics",
  "description": "An extension exposes metrics for Prometheus with LoopBack 4",
  "version": "0.7.0",
  "keywords": [
    "LoopBack",
    "Cloud Native",
    "Prometheus",
    "Metrics"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp.",
  "copyright.owner": "IBM Corp.",
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
    "directory": "extensions/metrics"
  },
  "engines": {
    "node": "^10.16 || 12 || 14 || 15"
  },
  "scripts": {
    "build": "lb-tsc",
    "clean": "lb-clean loopback-extension-metrics*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "verify": "npm pack && tar xf loopback-extension-metrics*.tgz && tree package && npm run clean"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__",
    "templates"
  ],
  "peerDependencies": {
    "@loopback/core": "^2.15.0",
    "@loopback/rest": "^9.2.0"
  },
  "dependencies": {
    "prom-client": "^13.1.0",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.3.0",
    "@loopback/eslint-config": "^10.1.0",
    "@loopback/testlab": "^3.3.0",
    "@types/express": "^4.17.11",
    "@types/node": "^10.17.56",
    "express": "^4.17.1"
  }
}
